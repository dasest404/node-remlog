{
	"name": "@namics/remlog-browser-client",
	"version": "0.1.10",
	"description": "",
	"main": "index.js",
	"scripts": {
		"test": "ava --tap | tap-diff",
		"test:watch": "ava --no-cache --watch",
		"build": "rollup -c",
		"dev": "rollup -c -w",
		"commit": "./node_modules/.bin/git-cz",
		"generate-changelog": "./node_modules/.bin/conventional-changelog -p angular -i CHANGELOG.md -s  -r 0 --lernaPackage @namics/remlog-browser-client && git add CHANGELOG.md"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/namics/node-remlog.git"
	},
	"keywords": [
		"remote",
		"logging",
		"remlog",
		"node",
		"debug",
		"transport",
		"@namics/remlog",
		"remotely",
		"server"
	],
	"author": "Jan Biasi <biasijan@gmail.com>",
	"license": "MIT",
	"publishConfig": {
		"access": "public"
	},
	"dependencies": {
		"@namics/remlog-utils": "0.1.10"
	},
	"devDependencies": {
		"@babel/core": "^7.0.0-beta.38",
		"@babel/preset-env": "^7.0.0-beta.38",
		"ava": "0.24.0",
		"babel-cli": "^6.26.0",
		"babel-minify": "^0.2.0",
		"babel-plugin-transform-class-properties": "^6.24.1",
		"browser-env": "3.2.4",
		"commitizen": "2.9.6",
		"conventional-changelog-cli": "1.3.5",
		"cz-conventional-changelog": "2.1.0",
		"expose-loader": "0.7.4",
		"rollup": "^0.55.0",
		"rollup-plugin-babel": "^3.0.3",
		"rollup-plugin-json": "^2.3.0",
		"rollup-plugin-node-resolve": "^3.0.2",
		"rollup-plugin-uglify": "^3.0.0",
		"rollup-watch": "^4.3.1",
		"tap-diff": "0.1.1"
	},
	"ava": {
		"babel": "inherit",
		"require": [
			"babel-register",
			"./test/helpers/setup-browser-env.js"
		]
	},
	"babel": {
		"presets": [
			"@ava/stage-4"
		]
	}
}
