{
    "devDependencies": {
        "commitizen": "^2.9.6",
        "conventional-changelog": "^1.1.7",
        "conventional-changelog-cli": "^1.3.5",
        "cz-conventional-changelog": "^2.1.0",
        "lerna": "2.0.0",
        "lerna-terminal": "1.1.1",
        "prettier": "1.10.2"
    },
    "name": "remlog",
    "version": "0.1.0",
    "main": "index.js",
    "repository": "https://github.com/janbiasi/remlog.git",
    "author": "Jan Biasi <biasijan@gmail.com>",
    "license": "MIT",
    "private": false,
    "scripts": {
        "test": "./node_modules/.bin/lerna-terminal test",
        "clean-package-lock": "rm -rf ./**/*package-lock.json",
        "precommit": "./hooks/prettier.sh",
        "commit": "./node_modules/.bin/git-cz",
        "generate-changelog":
            "./node_modules/.bin/conventional-changelog -p remlog -i CHANGELOG.md -s && git add CHANGELOG.md"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    }
}
